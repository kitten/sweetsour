{
  "name": "@sweetsour/parser",
  "version": "0.1.0-1",
  "source": "./index.js",
  "umd:main": "./dist/sweetsour-parser.js",
  "main": "./dist/sweetsour-parser.cjs.js",
  "jsnext:main": "./dist/sweetsour-parser.es.js",
  "module": "./dist/sweetsour-parser.es.js",
  "scripts": {
    "clean": "bsb -clean-world",
    "build": "bsb -make-world",
    "watch": "bsb -make-world -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "precommit": "lint-staged",
    "lint-staged": "lint-staged",
    "dist": "rollup -c ../../rollup.config.js",
    "prepublishOnly": "run-s build dist"
  },
  "keywords": [
    "bucklescript",
    "reason",
    "sweetsour",
    "ISTF",
    "CSS",
    "parser",
    "css-in-js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "bs-jest": "^0.3.2",
    "bs-loader": "^2.0.1",
    "bs-platform": "^3.1.4",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "npm-run-all": "^4.1.2",
    "refmt": "^1.13.7-1",
    "rollup": "^0.50.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "re",
      "js",
      "ml"
    ],
    "testMatch": [
      "**/__tests__/*_test.re",
      "**/__tests__/suite/*_test.re"
    ],
    "transform": {
      ".(re|ml)": "bs-loader"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env"
          ]
        ]
      }
    }
  },
  "lint-staged": {
    "*.re": [
      "jest --bail --findRelatedTests"
    ]
  }
}
